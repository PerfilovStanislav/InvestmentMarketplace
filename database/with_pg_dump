--
-- Name: deleteproject(); Type: FUNCTION; Schema: public; Owner: postgres
--

CREATE FUNCTION deleteproject() RETURNS trigger
    LANGUAGE plpgsql
    AS $$
BEGIN
    DELETE FROM "project_lang"
    WHERE "project_lang"."project_id" = OLD."id";
    RETURN OLD;        
END;
$$;


ALTER FUNCTION public.deleteproject() OWNER TO postgres;

